/*EATE FUNCTION Main() RETURNS BOOLE*/
	BEGIN
		
		SET Environment.Variables.Emp_Details = (SELECT T.* FROM DataSource.DB2SCHEMA.EMPLOYMENT_DETAILS  AS T
		WHERE T.EMPNAME = CAST(InputRoot.XMLNSC.emp.emp_name as CHARACTER) AND T.EMPNO = CAST(InputRoot.XMLNSC.emp.emp_no AS INTEGER));
		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
