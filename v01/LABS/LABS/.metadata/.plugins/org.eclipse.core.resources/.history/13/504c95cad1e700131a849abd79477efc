DECLARE ns NAMESPACE 'http://addressbook.com/';


CREATE COMPUTE MODULE RoutingCompute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		SET OutputRoot = InputRoot;
		
		SET OutputLocalEnvironment.Destination.RouterList.DestinationData[1].labelname = InputRoot.XMLNSC.request.operataion;
		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;


CREATE COMPUTE MODULE ComputeResponse
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		SET OutputRoot.Properties = InputRoot.Properties;
		
		SET OutputRoot.SOAP.Body = InputRoot.SOAP.Body;
		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;


CREATE COMPUTE MODULE saveCompute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		SET OutputRoot.Properties.CodedCharSetId = 1208;
		
		DECLARE request REFERENCE TO InputRoot.XMLNSC.request;
		
		SET OutputRoot.SOAP.Body.ns:Name =request.Name;
		SET OutputRoot.SOAP.Body.ns:Address.ns:City = request.City;
		SET OutputRoot.SOAP.Body.ns:Address.ns:Street = request.Street;
		SET OutputRoot.SOAP.Body.ns:Address.ns:Province = request.Province;
		SET OutputRoot.SOAP.Body.ns:Address.ns:PostalCode = request.PostalCode;
		SET OutputRoot.SOAP.Body.ns:Address.ns:PhoneNumber.ns:Area = request.Area;
		SET OutputRoot.SOAP.Body.ns:Address.ns:PhoneNumber.ns:Prefix = request.Prefix;
		SET OutputRoot.SOAP.Body.ns:Address.ns:PhoneNumber.ns:Local = request.Local;
		
		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;


CREATE COMPUTE MODULE findCompute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		SET OutputRoot.Properties.CodedCharSetId = 1208;
		
		
		SET OutputRoot.SOAP.Body.ns:Name = InputRoot.XMLNSC.request.Name;
		
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyMessageHeaders() BEGIN
		DECLARE I INTEGER 1;
		DECLARE J INTEGER;
		SET J = CARDINALITY(InputRoot.*[]);
		WHILE I < J DO
			SET OutputRoot.*[I] = InputRoot.*[I];
			SET I = I + 1;
		END WHILE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET OutputRoot = InputRoot;
	END;
END MODULE;
