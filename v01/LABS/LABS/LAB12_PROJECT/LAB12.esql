CREATE COMPUTE MODULE LAB12_Compute2
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		SET OutputRoot.MQMD = NULL;
		SET OutputRoot.XMLNSC.ns:submitPOResponse.orderStatus = InputRoot.XMLNSC.OrderResponse.Status;
		SET OutputRoot.XMLNSC.ns:submitPOResponse.orderAmt = InputRoot.XMLNSC.OrderResponse.AMT;
		SET OutputRoot.XMLNSC.ns:submitPOResponse.partNo = InputRoot.XMLNSC.OrderResponse.PartNo;
